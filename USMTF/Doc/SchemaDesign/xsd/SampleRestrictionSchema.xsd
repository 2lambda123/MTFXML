    <xs:complexType name="ChoiceAlternativeContentType">
        <xs:annotation>
            <xs:documentation ism:classification="U" ism:ownerProducer="USA" ism:noticeType="DoD-Dist-A">A data type for alternative content.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0">
                <xs:annotation>
                    <xs:documentation ism:classification="U" ism:ownerProducer="USA" ism:noticeType="DoD-Dist-A">A data concept for a choice of a data item for the instructions for acknowledging or the force and/or unit required to acknowledge.</xs:documentation>
                    <xs:appinfo>
                        <inf:Field positionName="ALTERNATIVE CONTENT" type="AlternativeContentAbstractType" fieldseq="2"/>
                        <inf:Choice substitutionGroup="AlternativeContentAbstract">
                            <inf:Element name="TextAlternative" type="FieldTextType"/>
                            <inf:Element name="EnumerationAlternative" type="EnumerationCodeType"/>
                            <inf:Element name="IntegerAlternative" type="IntegerType"/>
                            <inf:Element name="DecimalAlternative" type="DecimalType"/>
                            <inf:Element name="CompositeAlternative" type="CompositeType"/>
                            <inf:Element name="SetAlternative" type="SetType"/>
                            <inf:Element name="SegmentAlternative" type="SegmentType"/>
                        </inf:Choice>
                    </xs:appinfo>
