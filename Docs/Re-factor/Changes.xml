    <Fields>
        <FieldStringBaseType>
            <Baseline>
                <simpleType name="MissionNumberType">
                    <annotation/>
                    <restriction base="string">
                        <annotation/>
                        <minLength value="1"/>
                        <maxLength value="8"/>
                        <pattern value="[\-\.,\(\)\?A-Z0-9&#x20;]{1,8}"/>
                    </restriction>
                </simpleType>
                <element name="MissionNumber" type="MissionNumberType"/>
            </Baseline>
            <Refactor>
                <complexType name="AlphaNumericBlankSpecialTextType">
                    <simpleContent>
                        <restriction base="FieldStringBaseType">
                            <pattern value="[\-\.,\(\)\?A-Z0-9 ]+"/>
                        </restriction>
                    </simpleContent>
                </complexType>
                <element name="MissionNumber">
                    <annotation/>
                    <complexType>
                        <simpleContent>
                            <restriction base="AlphaNumericBlankSpecialTextType">
                                <minLength value="1"/>
                                <maxLength value="8"/>
                            </restriction>
                        </simpleContent>
                    </complexType>
                </element>
            </Refactor>
        </FieldStringBaseType>
        <FieldEnumerationType>
            <Baseline>
                <simpleType name="PersistentIndicatorType">
                    <annotation/>
                    <restriction base="string">
