                                </complexContent>
                            </complexType>
                        </element>
                        <element name="GridLocation" minOccurs="0" maxOccurs="1">
                            <complexType>
                                <complexContent>
                                    <extension base="s:GridLocationType">
                                        <attribute name="setSeq" type="unsignedShort" fixed="53"/>
                                    </extension>
                                </complexContent>
                            </complexType>
                        </element>
                        <element name="Line" minOccurs="0" maxOccurs="1">
                            <complexType>
                                <complexContent>
                                    <extension base="s:LineType">
                                        <attribute name="setSeq" type="unsignedShort" fixed="54"/>
                                    </extension>
                                </complexContent>
                            </complexType>
                        </element>
                        <element name="TimeOfReportedUnitLocation" minOccurs="0" maxOccurs="1">
                            <complexType>
                                <complexContent>
                                    <extension base="s:TimeType">
                                        <attribute name="setSeq" type="unsignedShort" fixed="55"/>
                                    </extension>
                                </complexContent>
                            </complexType>
                        </element>
                    </sequence>
                    <attribute name="segSeq" type="unsignedShort" fixed="51"/>
                </complexType>
            </element>
        </Baseline>
        <Refactor>
            <complexType name="ReportedUnitLocationSegmentType">
                <sequence>
                    <element ref="set:OrderLocationType"/>
                    <element ref="set:Area"/>
                    <element ref="set:GridLocation"/>
                    <element ref="set:Line"/>
                    <element name="TimeOfReportedUnitLocation" minOccurs="0" maxOccurs="1" type="set:TimeType"/>
                </sequence>
            </complexType>
        </Refactor>
    </Segments>
    <Messages> </Messages>
</USMTF_Refactor>
