    <xs:annotation>
        <xs:documentation ism:classification="U"
            ism:ownerProducer="USA"
            ism:noticeType="DoD-Dist-A">A data item for the TEST is used to report on the status of a TEST.</xs:documentation>
        <xs:appinfo>
            <inf:Msg name="TEST MESSAGE"
                mtfid="TEST"
                sponsor="USAF"
                purpose="The TEST is used to report on the status of a new or established air base indicating the capability of the air base and its supporting facilities to support joint operations and to conduct planned or anticipated operations."
                MtfRemark="B.1.01.13 was updated by ICP M2017-04, which updated the REF Set."
                version="C.0.01.00"
                date="October 2018"
                remark="Created by ICP M2018-02"/>
            <inf:StructuralRelationships>
                <sch:schema queryBinding="xslt">
                    <sch:ns uri="urn:mtf:mil:6040b:niem:mtf" prefix="mtf"/>
                    <sch:ns uri="urn:us:gov:ic:ism" prefix="ism"/>
                    <sch:title>Structural Relationship Rules</sch:title>
                    <sch:pattern id="TestMessage">
                        <sch:title>Structural Relationship Rules</sch:title>
                        <sch:rule context="mtf:TestMessage/mtf:MessageIdentifierSet">
                            <sch:assert fpi="[3]F1 A /TEST/"
                                test="mtf:MessageTextFormatIdentifierText = 'TEST'">Field 1 in MSGID (Set 3) is assigned the value "TEST".</sch:assert>
                            <sch:assert fpi="[3]F2 A /MIL-STD-6040(SERIES)/"
                                test="mtf:StandardOfMessageTextFormatCode = 'MIL-STD-6040(SERIES)'">Field 2 in MSGID (Set 3) is assigned the value "MIL-STD-6040(SERIES)".</sch:assert>
                            <sch:assert fpi="[3]F3 A /B.1.01.14/"
                                test="mtf:VersionOfMessageTextFormatText = 'B.1.01.14'">Field 3 in MSGID (Set 3) is assigned the value "B.1.01.14".</sch:assert>
                        </sch:rule>
                        <sch:rule context="mtf:TestMessage/mtf:ReferenceSet">
                            <sch:assert fpi="[4],AF3 R ([4],AF2 EQ /CHT/ | /DOC/)"
                                test="mtf:InformationProduct and (mtf:TypeCommunicationCode='CHT' or mtf:TypeCommunicationCode='DOC')">Field 3 in REF (Set 4) is required if Field 2 in the same REF (Set 4) lexicographically equals "CHT" or "DOC".</sch:assert>
                        </sch:rule>
                        <sch:rule context="mtf:TestMessage/mtf:SetOne">
                            <sch:assert fpi="[6],AF2 R ([6],AF3 !@)"
                                test="mtf:CompositeOne and not(mtf:CompositeTwo)">Field 1 in SETONE (Set 6) is required if Field 2 in the same SETONE (Set 6) does not occur.</sch:assert>
                        </sch:rule>
                    </sch:pattern>
                </sch:schema>
            </inf:StructuralRelationships>
        </xs:appinfo>
    </xs:annotation>


    <xs:simpleType name="FieldTextSimpleType">
        <xs:annotation>
            <xs:documentation ism:classification="U" ism:ownerProducer="USA"
                ism:noticeType="DoD-Dist-A">A data type for the field text.</xs:documentation>
            <xs:appinfo>
                <inf:Field name="FIELD" version="C.0.01.00" ffirn="000" fud="00"
                    type="FieldTextType"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[\-\.,\(\)\?A-Z0-9 ]{1,38}"/>
            <xs:minLength value="1"/>
            <xs:maxLength value="38"/>
        </xs:restriction>
    </xs:simpleType>
