    <Fields>
        <Strings>
            <Baseline>
                <simpleType name="MissionNumberType">
                    <annotation/>
                    <restriction base="string">
                        <annotation/>
                        <minLength value="1"/>
                        <maxLength value="8"/>
                        <pattern value="[\-\.,\(\)\?A-Z0-9&#x20;]{1,8}"/>
                    </restriction>
                </simpleType>
                <element name="MissionNumber" type="MissionNumberType"/>
            </Baseline>
            <Refactor>
                <complexType name="AlphaNumericBlankSpecialTextType">
                    <simpleContent>
                        <restriction base="FieldStringBaseType">
                            <pattern value="[\-\.,\(\)\?A-Z0-9 ]+"/>
                        </restriction>
                    </simpleContent>
                </complexType>
                <element name="MissionNumber">
                    <annotation/>
                    <complexType>
                        <simpleContent>
                            <restriction base="AlphaNumericBlankSpecialTextType">
                                <minLength value="1"/>
                                <maxLength value="8"/>
                            </restriction>
                        </simpleContent>
                    </complexType>
                </element>
            </Refactor>
        </Strings>
        <Enumerations>
            <Baseline>
                <simpleType name="PersistentIndicatorType">
                    <annotation/>
                    <restriction base="string">
                        <enumeration value="YES">
                            <annotation/>
                        </enumeration>
                        <enumeration value="NO">
                            <annotation/>
                        </enumeration>
                    </restriction>
                </simpleType>
                <element name="PersistentIndicator" type="YesNoType"/>
            </Baseline>
            <Refactor>
                <complexType name="YesNoType">
                    <simpleContent>
                        <restriction base="FieldEnumeratedBaseType">
                            <enumeration value="YES">
                                <annotation>
                                    <appinfo>
                                        <Enum dataItem="AFFIRMATIVE INDICATOR"/>
                                    </appinfo>
                                </annotation>
                            </enumeration>
                            <enumeration value="NO">
                                <annotation>
                                    <appinfo>
                                        <Enum dataItem="NEGATIVE INDICATOR"/>
                                    </appinfo>
                                </annotation>
                            </enumeration>
                        </restriction>
                    </simpleContent>
                </complexType>
                <element name="PersistentIndicator" type="YesNoType"/>
            </Refactor>
        </Enumerations>
        <Integers>
            <Baseline>
                <simpleType name="ElevationMslType">
                    <restriction base="xsd:integer">
                        <minInclusive value="-9999"/>
                        <maxInclusive value="99999"/>
                        <pattern value="[\-0-9]{5}"/>
                    </restriction>
                </simpleType>
                <element name="ElevationMsl" type="ElevationMslType"/>
            </Baseline>
            <Refactor>
                <element name="ElevationMsl">
                    <complexType>
                        <simpleContent>
                            <restriction base="FieldIntegerBaseType">
                                <minInclusive value="-9999"/>
                                <maxInclusive value="99999"/>
                            </restriction>
                        </simpleContent>
                    </complexType>
                </element>
            </Refactor>
        </Integers>
        <Decimals>
            <Baseline>
                <simpleType name="FlightTimeInHoursAndTenthsType">
                    <restriction base="xsd:decimal">
                        <minInclusive value="0.1"/>
                        <maxInclusive value="24"/>
                        <pattern value="[\-0-9]{2}\.[0-9]{1}"/>
                    </restriction>
                </simpleType>
                <element name="FlightTimeInHoursAndTenths" type="FlightTimeInHoursAndTenthsType"/>
            </Baseline>
            <Refactor>
                <element name="FlightTimeInHoursAndTenths">
                    <complexType>
                        <simpleContent>
                            <restriction base="FieldDecimalBaseType">
                                <minInclusive value="0.1"/>
                                <maxInclusive value="24"/>
                                <fractionDigits value="1"/>
                                <totalDigits value="3"/>
                            </restriction>
                        </simpleContent>
                    </complexType>
                </element>
            </Refactor>
        </Decimals>
    </Fields>
    <Sets>
        <Baseline>
            <complexType name="EventIdentificationType">
                <complexContent>
                    <extension base="SetBaseType">
                        <sequence>
                            <element name="EventKey" minOccurs="1" maxOccurs="1" nillable="true">
                                <complexType>
                                    <complexContent>
                                        <extension base="c:EventKeyType">
                                            <attribute name="ffSeq" type="unsignedShort" fixed="1"/>
                                            <attribute name="ffirnFudn" type="string" fixed="FF2444-1"/>
                                        </extension>
                                    </complexContent>
                                </complexType>
                            </element>
                            <element name="EventName" minOccurs="1" maxOccurs="1" nillable="true">
                                <complexType>
                                    <simpleContent>
                                        <extension base="f:EventNameType">
                                            <attribute name="ffSeq" type="unsignedShort" fixed="2"/>
                                            <attribute name="ffirnFudn" type="string" fixed="FF1022-254"/>
                                        </extension>
                                    </simpleContent>
                                </complexType>
                            </element>
                            <element name="Remarks" minOccurs="0" maxOccurs="1">
                                <complexType>
                                    <simpleContent>
                                        <extension base="f:RemarksType">
                                            <attribute name="ffSeq" type="unsignedShort" fixed="3"/>
                                            <attribute name="ffirnFudn" type="string" fixed="FF1357-28"/>
                                        </extension>
                                    </simpleContent>
                                </complexType>
                            </element>
                        </sequence>
                        <attribute name="setid" type="string" fixed="EVENTID"/>
                    </extension>
                </complexContent>
            </complexType>
        </Baseline>
        <ReFactor>
            <complexType name="EventIdentificationType">
                <annotation>
                    <appinfo>
                        <Set name="EVENT IDENTIFICATION" id="EVENTID" version="1.0">
                            <Example>EVENTID/E-2001-GE22-00001/EXERCISE DRAGON HEAD/REMARKS(LITERAL)//</Example>
                        </Set>
                    </appinfo>
                </annotation>
                <complexContent>
                    <extension base="SetBaseType">
                        <sequence>
                            <element name="EventKey" minOccurs="1" maxOccurs="1" nillable="true">
                                <annotation>
                                    <appinfo>
                                        <Field positionName="EVENT KEY" identifier="A" alignment="Left"
                                            usage="ENTER THE UNIQUE IDENTIFICATION FOR EACH SINGLE EVENT CLASSIFICATION, FOR EXAMPLE: E-2001-GE22-00001."
                                            name="EVENT KEY" definition="A UNIQUE IDENTIFICATION OF A SPECIFIC EVENT ROOT RECORD." version="1.0"/>
                                    </appinfo>
                                </annotation>
                                <complexType>
                                    <complexContent>
                                        <extension base="field:EventKeyType">
                                            <annotation>
                                                <documentation>A UNIQUE IDENTIFICATION OF A SPECIFIC EVENT ROOT RECORD.</documentation>
                                                <appinfo>
                                                    <Field name="EVENT KEY" definition="A UNIQUE IDENTIFICATION OF A SPECIFIC EVENT ROOT RECORD."
                                                        version="1.0">
                                                        <Document>AINTP-3</Document>
                                                        <Document>STANAG 2441</Document>
                                                    </Field>
                                                </appinfo>
                                            </annotation>
                                        </extension>
                                    </complexContent>
                                </complexType>
                            </element>
                            <element name="EventName" minOccurs="1" maxOccurs="1" nillable="true">
                                <annotation>
                                    <appinfo>
                                        <Field positionName="EVENT NAME" identifier="A" alignment="Left"
                                            usage="ENTER THE AGREED NAME OF AN EVENT, FOR EXAMPLE: EXERCISE DRAGON HEAD." name="NAME"
                                            definition="AN IDENTIFIER WHICH IS COMMONLY CONSIDERED TO BE A NAME OF AN ENTITY." version="2.0"/>
                                    </appinfo>
                                </annotation>
                                <complexType>
                                    <simpleContent>
                                        <restriction base="field:AlphaNumericBlankSpecialInitDataLoadIDType">
                                            <minLength value="1"/>
                                            <maxLength value="54"/>
                                        </restriction>
                                    </simpleContent>
                                </complexType>
                            </element>
                            <element ref="field:Remarks">
                                <annotation>
                                    <appinfo>
                                        <Field positionName="REMARKS" identifier="A" alignment="Left"
                                            usage="ENTER ANY OTHER DATA (LITERAL UP TO 68 CHARACTERS) WHICH IS NOT ALREADY COVERED IN THE SPECIFIC FIELDS."
                                            name="COMMENTS" definition="A BRIEF NOTE OR TEXTUAL."
                                            remark="COMMENTS LONGER THAN CAN BE ACCOMMODATED BY THIS FIELD FORMAT SHOULD BE ADDRESSED IN AN 'AMPN' SET."
                                            version="2.0"/>
                                    </appinfo>
                                </annotation>
                            </element>
                        </sequence>
                        <attribute name="setid" type="string" fixed="EVENTID"/>
                    </extension>
                </complexContent>
            </complexType>
        </ReFactor>
    </Sets>
    <Segments>
        <Baseline>
            <element name="ReportedUnitLocationSegment" minOccurs="0" maxOccurs="unbounded">
                <complexType>
                    <sequence>
                        <element name="OrderLocationType" minOccurs="1" maxOccurs="1">
                            <complexType>
